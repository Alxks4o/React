extends layout 

block content 
    h1 Users 

    table 
        tr 
            th Email Address 
            th Change Password 
            th Delete User 
        each user in users 
            tr 
                td email@email.com
                td 
                    a(href='/users/'+user._id+'/password')
                        button(type="button") Change Password 
                td 
                    a(href='/users/')
                        button(type="button") Delete User 
        